<h1>Fruit CSV Exporter 3000</h1>

<%= form_tag(fruits_export_csv_path, method: :get, "data-turbo": false) do %>
  Name: <%= text_field_tag :name %>
  Family: <%= text_field_tag :family %>
  Genus: <%= text_field_tag :genus %>
  Order: <%= text_field_tag :order %>
  Fruit ID: <%= text_field_tag :fruit_id %>
  Filename: <%= text_field_tag :filename %>
  <%= submit_tag 'Export to CSV' %>
<% end %>
